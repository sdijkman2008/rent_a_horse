<h1>Horses</h1>
<div class="container">
  <div class="row">
    <div class="text-center">
      <%= link_to "Add a horse", new_horse_path, class: "btn btn-primary btn-lg" %>
    </div>
  </div>
</div>

<% @horses.each do |horse| %>
  <h2><%= link_to horse.name, horse_path(horse) %></h2>
  <% end %>



